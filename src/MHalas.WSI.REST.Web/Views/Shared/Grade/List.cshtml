@model List<MHalas.WSI.REST.Models.Grade>

<p>
    @Html.ActionLink("Create New", "Create", "Grade")
</p>
<table class="table">
    <tr>
        <th>
            Added date:
        </th>
        <th>
            Grade
        </th>
        <th>
            Course name:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AddedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GradeValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseName)
        </td>
    </tr>
}

</table>
